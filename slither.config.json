{
  "detectors_to_exclude": "missing-zero-check,initialize-target,tx-origin,assembly,reentrancy-events,unused-return,naming-convention,timestamp",
  "disable_color": false,
  "filter_paths": "(lib/|node_modules/|upgradeability/|mocks|/test/|script/)",
